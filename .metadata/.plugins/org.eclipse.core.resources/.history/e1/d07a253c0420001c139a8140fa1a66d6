package Arrays;

public class ViDu {
	
	public static void main(String[] args) {
		String names[] = new String[3];
		names[0] = "mai";
		names[1] = "xuan";
		//System.out.println(names);
		
		//======== cach khai bao 2
		String [] na = {"Mai", "xuan"};
		String[] nbStrings = na;
		for(int i=0; i<na.length;i++)
		{
			System.out.print (na[i]+" ");
		}
		//======== cach khai bao 3
				String nb[] = new String[] {"Mai" ,"Xuan"}; 
				
				
				
				//===============CACH GAN 
				//c1 gan truc tiep
				String[] nbStrings = na;
				for(int i=0; i<na.length;i++)
				{
					System.out.print ("Mang sau khi gan "+nbStrings[i]+" ");
				}
				
				
	}
	

}
