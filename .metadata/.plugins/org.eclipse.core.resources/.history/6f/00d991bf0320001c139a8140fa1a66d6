package Arrays;

public class ViDu {
	
	public static void main(String[] args) {
		String names[] = new String[3];
		names[0] = "mai";
		names[1] = "xuan";
		System.out.println(names);
		
		//======== cach khai bao 2
		String na[] = {"Mai", "xuan"};
		//======== cach khai bao 3
				String nb[] = new String[] {"Mai" ,"Xuan"}; 
	}
	

}
