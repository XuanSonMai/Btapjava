package Mythread;

public class TestThread2 implements Runnable{
	public int Tong;
	
	
	public TestThread2()
	{
		
		this.Tong=1000;
	}
	
	public void rutTien()
	{
		if(Tong>0)
		{
			Tong = Tong-1000;
		}
		else {
			System.out.println("ko con tien");
		}
	}
	@Override
	public void run() {
		
		
	}

}
